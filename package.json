{
  "name": "yeri",
  "version": "0.9.0",
  "description": "draw full-color (text or SVG) time-series graphs",
  "keywords": [ "graphite", "graph", "iyeriwok" ],
  "author": "Robey Pointer <robeypointer@gmail.com>",
  "homepage": "https://github.com/robey/yeri",
  "repository": {
    "type": "git",
    "url": "git://github.com/robey/yeri.git"
  },
  "licenses": [
    {
      "type": "Apache License 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.txt"
    }
  ],
  "scripts": {
    "prepublish": "6to5 --source-maps --out-dir lib src && 6to5 --source-maps --out-dir test/lib test/src",
    "test": "mocha -R spec --colors test/lib"
  },
  "dependencies": {
    "antsy": "*",
    "bluebird": "*",
    "lodash": "*",
    "optimist": "*",
    "request": "*",
    "strftime": "*"
  },
  "devDependencies": {
    "6to5": "*",
    "mocha": "*",
    "should": "*",
    "source-map-support": "*"
  },
  "main": "./lib/yeri",
  "files": [
    "build.plz",
    "lib",
    "bin/yeri",
    "test"
  ],
  "bin": {
    "yeri": "bin/yeri"
  },
  "preferGlobal": "true"
}
